package projects;

public class StoreEvenNum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	
		
		int [] array = new int[51];                       // Sorted Array of 100
		for (int a = 0; a < array.length; a = a + 2) {
		    array[a] = a + 1;
		    System.out.println(a);
		}

	}

}
